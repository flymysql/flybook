{
  "name": "express-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "pmstart": "pm2 start ./bin/www --name=\"blog\"  --ignore-watch=\"^(?!.*config.js).*$\"",
    "pmstop": "pm2 stop all",
    "pmrestart": "./node_modules/.bin/pm2 restart all --ignore-watch=\"logs node_modules server sessions public/uploads\"",
    "update": "sh update.sh"
  },
  "watch": [
    "./config.js"
  ],
  "dependencies": {
    "async": "^2.6.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.16.1",
    "http-errors": "~1.6.3",
    "lowdb": "^1.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "nodemailer": "^6.4.2",
    "nodemon": "^1.19.0",
    "pm2": "^3.5.0",
    "pug": "2.0.0-beta11",
    "session-file-store": "^1.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
